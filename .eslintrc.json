{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            "error",
            {
                "code": 80
            }
        ],
        "indent": [
            "error",
            2,
            {
                "SwitchCase": 1
            }
        ]
    }
}